import { Button, VerticalBox, ListView, TabWidget } from "std-widgets.slint";

export component AppWindow inherits Window {
	in-out property<[string]> lines: [];

	callback line-clicked(int);
	ListView {
		for line[i] in lines : Text {
			text: line;
			if line != "" : TouchArea {
				clicked => {
					root.line-clicked(i);
				}
			}
		}
	}
}
